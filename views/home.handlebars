<div id="jumbotron" class="jumbotron jumbotron-fluid d-flex text-center ">
  <div class="container  ">
    <h1 class="display-4 ">Coding ; Life </h1>
    <p class="lead">This is a modified jumbotron that occupies the entire horizontal space of its parent.</p>
  </div>
</div>

<div class="container  ">
  {{#each array}}
  <div class="card-deck container-card">
    <div class="card main-card  " style="max-width: 40rem;">
      <img class="card-img-top"
        src="{{this.url}}"
        alt="Card image cap">
      <div class="card-body">
        <h5 class="card-title">{{this.title}}</h5>
        <p class="card-text">{{this.text}}</p>
        <p class="card-text"><small class="text-muted">{{this.date}}</small></p>
      </div>
    </div>
  </div>
  {{/each}}
</div>




